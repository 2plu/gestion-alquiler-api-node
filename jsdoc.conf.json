{
    "plugins": [
        "docdash"
    ],
    "recurseDepth": 10,
    "opts": {
        "recurse": true,
        "destination": "./docs/",
        "template": "node_modules/docdash"
    },
    "source": {
        "include": [
            "."
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "node_modules"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": ["category"],
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}